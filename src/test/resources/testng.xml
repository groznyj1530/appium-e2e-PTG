<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Testing Suite">
    <groups>
        <run>
            <exclude name="regression"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="configs.Listener"/>
    </listeners>
    <test name="smoke">
        <packages>
            <package name="android.tests"/>
        </packages>
    </test>
</suite>